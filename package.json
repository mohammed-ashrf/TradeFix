    "package-linux": "electron-packager . Al-Ahram-setup — overwrite — asar=true — platform=linux — arch=x64 — icon=src/assets/icons/png/repairing.png — prune=true — out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
